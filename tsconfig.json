{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "importHelpers": true,
    "declaration": false,
    "removeComments": false,
    "noImplicitAny": false,
    "inlineSources": true,
    "skipLibCheck": true,
    "outDir": "tmp",
    "typeRoots": [
      "types",
      "./node_modules/@types"
    ],
    "types": [
      "node",
      "jasmine"
    ]
  },
  "include": [
    "protractor.conf.ts"
  ]
}
